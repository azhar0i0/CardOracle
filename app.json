{
  "expo": {
    "name": "CardOrcale",
    "slug": "CardOrcale",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "nativewind",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#1A0F2E",
        "foregroundImage": "./assets/images/icon.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.azhar0i0.CardOrcale"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/icon.png"
    },
    "plugins": [
  "expo-router",
  [
    "expo-splash-screen",
    {
      "image": "./assets/images/icon.png",
      "imageWidth": 180,
      "resizeMode": "cover",
      "backgroundColor": "#1A0F2E",
      "dark": {
        "backgroundColor": "#1A0F2E"
      }
    }
  ]
],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "772614b6-6919-4066-b294-2c409e740494"
      }
    }
  }
}
